  <Sequence DisplayName="Sequence_Sum" sap2010:WorkflowViewState.IdRef="Sequence_2">
    <Sequence.Variables>
      <Variable x:TypeArguments="x:String" Name="strNumberInput" />
      <Variable x:TypeArguments="x:String" Name="strNumberSequence" />
